{
  conn.reportBytesRead(bytesRead);
  bytesRead=0;
  request.reportResponseEnd(this);
  if (paused) {
    hasPausedEnd=true;
    pausedTrailer=trailer;
  }
 else {
    this.trailer=trailer;
    trailers=new HeadersAdaptor(trailer.trailingHeaders());
    if (endHandler != null) {
      endHandler.handle(null);
    }
  }
}
