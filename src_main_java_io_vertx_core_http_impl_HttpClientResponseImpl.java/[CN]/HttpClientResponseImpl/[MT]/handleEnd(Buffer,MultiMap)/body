{
synchronized (conn) {
    conn.connection().reportBytesRead(bytesRead);
    bytesRead=0;
    request.reportResponseEnd(this);
    if (paused) {
      pausedLastChunk=lastChunk;
      hasPausedEnd=true;
      pausedTrailers=trailers;
    }
 else {
      if (lastChunk != null) {
        handleChunk(lastChunk);
      }
      this.trailers=trailers;
      if (endHandler != null) {
        try {
          endHandler.handle(null);
        }
 catch (        Throwable t) {
          handleException(t);
        }
      }
    }
  }
}
