{
  ClassLoader cl=Thread.currentThread().getContextClassLoader();
  if (cl != null) {
    String spath=path.toString();
    String substituted=FILE_SEP == '/' ? spath : spath.replace(FILE_SEP,'/');
    URL url=cl.getResource(substituted);
    if (url != null) {
      Path thePath=urlToPath(url);
      if (thePath != null) {
        return thePath;
      }
    }
  }
  return path;
}
