{
  CLI command=CLIConfigurator.define(HelloClI.class);
  StringBuilder builder=new StringBuilder();
  command.usage(builder);
  assertThat(builder).isEqualToIgnoringCase("Usage: hello -n <name>\n" + "\n" + "A command saying hello.\n"+ "\n"+ "A simple cli to wish you a good day. Pass your name with `--name`\n\n"+ " -n,--name <name>   your name\n");
}
