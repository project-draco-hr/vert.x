{
synchronized (haMods) {
    Iterator<Object> iter=haMods.iterator();
    while (iter.hasNext()) {
      Object obj=iter.next();
      JsonObject mod=(JsonObject)obj;
      if (mod.getString("dep_id").equals(depID)) {
        iter.remove();
      }
    }
  }
  clusterMap.put(nodeID,haInfo.encode());
}
