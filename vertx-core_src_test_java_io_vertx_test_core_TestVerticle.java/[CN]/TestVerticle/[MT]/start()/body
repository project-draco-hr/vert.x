{
  processArgs=vertx.context().processArgs();
  conf=vertx.context().config();
  if (Thread.currentThread().getContextClassLoader() != getClass().getClassLoader()) {
    throw new IllegalStateException("Wrong tccl!");
  }
  vertx.eventBus().send("testcounts",new JsonObject().putString("deploymentID",vertx.context().deploymentID()).putNumber("count",instanceCount.incrementAndGet()));
}
