{
  this.vertx=vertx;
  this.orderedInternalPoolExec=orderedInternalPoolExec;
  EventLoopGroup group=vertx.getEventLoopGroup();
  if (group != null) {
    this.eventLoop=group.next();
    this.tccl=Thread.currentThread().getContextClassLoader();
  }
 else {
    this.eventLoop=null;
    this.tccl=null;
  }
}
