{
  SSLContext context=SSLContext.getInstance("TLS");
  context.init(null,null,null);
  SSLEngine engine=context.createSSLEngine();
  String[] expected=engine.getEnabledCipherSuites();
  SSLHelper helper=new SSLHelper(new HttpClientOptions(),KeyStoreHelper.create((VertxInternal)vertx,TLSCert.JKS.getClientKeyCertOptions()),KeyStoreHelper.create((VertxInternal)vertx,TLSCert.JKS.getClientTrustOptions()));
  SslContext ctx=helper.getContext((VertxInternal)vertx);
  assertEquals(new HashSet<>(Arrays.asList(expected)),new HashSet<>(ctx.cipherSuites()));
}
