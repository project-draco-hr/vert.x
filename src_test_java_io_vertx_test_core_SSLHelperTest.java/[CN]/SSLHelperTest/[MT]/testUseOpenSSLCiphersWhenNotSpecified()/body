{
  Set<String> expected=OpenSsl.availableCipherSuites();
  SSLHelper helper=new SSLHelper(new HttpClientOptions().setSslEngine(io.vertx.core.net.SSLEngine.OPENSSL),TLSCert.PEM.getClientKeyCertOptions(),TLSCert.PEM.getClientTrustOptions());
  SslContext ctx=helper.getContext((VertxInternal)vertx);
  assertEquals(expected,new HashSet<>(ctx.cipherSuites()));
}
