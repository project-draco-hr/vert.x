{
  JsonArray obj=new JsonArray();
  EventBus eb=vertx.eventBus();
  eb.registerHandler("foo",new Handler<Message<JsonArray>>(){
    @Override public void handle(    Message<JsonArray> msg){
      tu.azzert(msg.body().size() == 0);
      tu.testComplete();
    }
  }
);
  eb.send("foo",obj);
  obj.add("blah");
}
