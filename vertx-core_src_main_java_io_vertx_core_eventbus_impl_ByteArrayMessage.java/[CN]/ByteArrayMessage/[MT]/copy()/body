{
  byte[] bod;
  if (body != null) {
    bod=new byte[body.length];
    System.arraycopy(body,0,bod,0,bod.length);
  }
 else {
    bod=null;
  }
  ByteArrayMessage copied=new ByteArrayMessage(send,address,bod);
  copied.replyAddress=this.replyAddress;
  copied.bus=this.bus;
  copied.sender=this.sender;
  return copied;
}
