{
  if (conf != null) {
    return conf;
  }
  Config cfg=null;
  try (InputStream is=getConfigStream();InputStream bis=new BufferedInputStream(is)){
    if (is != null) {
      cfg=new XmlConfigBuilder(bis).build();
    }
  }
 catch (  IOException ex) {
    log.error("Failed to read config",ex);
  }
  return cfg;
}
