{
  this.vertx=vertx;
  this.conn=conn;
  this.version=request.getProtocolVersion();
  this.response=new DefaultHttpResponse(version,HttpResponseStatus.OK);
  this.keepAlive=version == HttpVersion.HTTP_1_1 || (version == HttpVersion.HTTP_1_0 && "Keep-Alive".equalsIgnoreCase(request.headers().get("Connection")));
}
