{
  if (searchDomains.isEmpty() || (hostname.length() > 0 && hostname.charAt(hostname.length() - 1) == '.')) {
    return false;
  }
 else {
    int idx=hostname.length();
    int dots=0;
    while (idx-- > 0) {
      if (hostname.charAt(idx) == '.') {
        dots++;
      }
      if (dots >= ndots) {
        return false;
      }
    }
    return true;
  }
}
