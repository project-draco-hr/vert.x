{
  if (FILE_SEP == '/') {
    String sfile=url.getFile();
    if (sfile != null) {
      return Paths.get(url.getFile());
    }
 else {
      return null;
    }
  }
 else {
    try {
      URI uri=url.toURI();
      if (uri.isOpaque()) {
        return Paths.get(url.getPath());
      }
 else {
        return Paths.get(uri);
      }
    }
 catch (    Exception exc) {
      throw new VertxException(exc);
    }
  }
}
