{
  checkContext();
  if (paused) {
    if (pendingData == null) {
      pendingData=new ArrayDeque<>();
    }
    pendingData.add(data);
    return;
  }
  if (dataHandler != null) {
    try {
      dataHandler.handle(data);
    }
 catch (    Throwable t) {
      handleHandlerException(t);
    }
  }
}
