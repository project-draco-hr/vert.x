{
  if (!ended) {
    ended=true;
    if (metric != null) {
      if (failed) {
        connection.metrics().requestReset(metric);
      }
 else {
        connection.reportBytesWritten(bytesWritten);
        connection.metrics().responseEnd(metric,this);
      }
    }
    return true;
  }
  return false;
}
