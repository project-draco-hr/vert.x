{
synchronized (conn) {
    if (statusMessage == null) {
switch (statusCode / 100) {
case 1:
        return "Informational";
case 2:
      return "Success";
case 3:
    return "Redirection";
case 4:
  return "Client Error";
case 5:
return "Server Error";
default :
return "Unknown Status";
}
}
return statusMessage;
}
}
