{
  if (host == null) {
  }
  this.metrics=metrics;
  this.metric=metric;
  this.stream_=stream_;
  this.vertx=vertx;
  this.ctx=ctx;
  this.connection=connection;
  this.encoder=encoder;
  this.stream=stream;
  this.push=push;
  this.host=host;
  if (contentEncoding != null) {
    putHeader(HttpHeaderNames.CONTENT_ENCODING,contentEncoding);
  }
}
