{
  DefaultHttpServerFileUpload upload=new DefaultHttpServerFileUpload(conn.vertx(),DefaultHttpServerRequest.this,name,filename,contentType,contentTransferEncoding,charset,size);
  NettyFileUpload nettyUpload=new NettyFileUpload(upload,name,filename,contentType,contentTransferEncoding,charset);
  if (uploadHandler != null) {
    uploadHandler.handle(upload);
  }
  return nettyUpload;
}
