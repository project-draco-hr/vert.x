{
  for (  ConnQueue queue : connQueues.values()) {
    queue.closeAllConnections();
  }
  connQueues.clear();
  for (  ConnQueue queue : connQueues2.values()) {
    queue.closeAllConnections();
  }
  connQueues2.clear();
  for (  ClientConnection conn : connectionMap.values()) {
    conn.close();
  }
  for (  Http2ClientConnection conn : connectionMap2.values()) {
    conn.close();
  }
}
