{
  connCount--;
  Waiter waiter=getNextWaiter();
  if (waiter != null) {
    createNewConnection(waiter);
  }
 else   if (connCount == 0) {
    mgr.queueMap.remove(address);
    if (ConnectionManager.this.metrics.isEnabled()) {
      ConnectionManager.this.metrics.closeQueue(address.host,address.port,metrics);
    }
  }
}
