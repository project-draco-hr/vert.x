{
  StringBuilder sb=new StringBuilder();
  Iterator<Map.Entry<String,String>> iterator=iterator();
  while (iterator.hasNext()) {
    Map.Entry<String,String> entry=iterator.next();
    sb.append(entry.getKey());
    sb.append(": ");
    sb.append(entry.getValue());
    sb.append("\n");
  }
  return sb.toString();
}
