{
synchronized (getLock()) {
    checkComplete();
    checkResponseHandler();
    if (conn != null) {
      if (!headWritten) {
        writeHead();
        if (completionHandler != null) {
          completionHandler.handle(conn.version());
        }
      }
    }
 else {
      connect(completionHandler);
      writeHead=true;
    }
    return this;
  }
}
