{
  System.out.println(("**** do close called !!"));
  super.close();
  if (heartbeatID != -1) {
    vertx.cancelTimer(heartbeatID);
  }
  if (timeoutTimerID != -1) {
    vertx.cancelTimer(timeoutTimerID);
  }
  if (id != null) {
    System.out.println(("**** removing session!!"));
    sessions.remove(id);
  }
  if (endHandler != null) {
    endHandler.handle(null);
  }
}
