{
  SocketChannel ch=javaChannel().accept();
  try {
    if (ch != null) {
      buf.add(new VertxNioSocketChannel(this,ch));
      return 1;
    }
  }
 catch (  Throwable t) {
    log.warn("Failed to create a new channel from an accepted socket.",t);
    try {
      ch.close();
    }
 catch (    Throwable t2) {
      log.warn("Failed to close a socket.",t2);
    }
  }
  return 0;
}
