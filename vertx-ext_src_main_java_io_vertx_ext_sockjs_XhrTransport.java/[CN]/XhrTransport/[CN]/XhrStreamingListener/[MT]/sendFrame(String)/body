{
  boolean hr=headersWritten;
  super.sendFrame(body);
  if (!hr) {
    req.response().writeBuffer(H_BLOCK);
  }
  String sbody=body + "\n";
  Buffer buff=Buffer.newBuffer(sbody);
  req.response().writeBuffer(buff);
  bytesSent+=buff.length();
  if (bytesSent >= maxBytesStreaming) {
    close();
  }
}
