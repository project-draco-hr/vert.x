{
  doTestDelimited(Buffer.buffer("start-a-b-c-dddabc"),"abc".getBytes(),new Integer[]{18},Buffer.buffer("start-a-b-c-ddd"));
  doTestDelimited(Buffer.buffer("start-abc-dddabc"),"abc".getBytes(),new Integer[]{18},Buffer.buffer("start-"),Buffer.buffer("-ddd"));
  doTestDelimited(Buffer.buffer("start-ab-c-dddabc"),"abc".getBytes(),new Integer[]{18},Buffer.buffer("start-ab-c-ddd"));
}
