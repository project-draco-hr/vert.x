{
  doTestDelimited(Buffer.newBuffer("start-a-b-c-dddabc"),"abc".getBytes(),new Integer[]{18},Buffer.newBuffer("start-a-b-c-ddd"));
  doTestDelimited(Buffer.newBuffer("start-abc-dddabc"),"abc".getBytes(),new Integer[]{18},Buffer.newBuffer("start-"),Buffer.newBuffer("-ddd"));
  doTestDelimited(Buffer.newBuffer("start-ab-c-dddabc"),"abc".getBytes(),new Integer[]{18},Buffer.newBuffer("start-ab-c-ddd"));
}
