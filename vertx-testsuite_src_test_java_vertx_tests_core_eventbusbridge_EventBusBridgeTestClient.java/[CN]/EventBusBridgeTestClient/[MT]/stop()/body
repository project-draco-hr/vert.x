{
  server.close(new Handler<AsyncResult<Void>>(){
    @Override public void handle(    AsyncResult<Void> result){
      if (result.succeeded()) {
        EventBusBridgeTestClient.super.stop();
      }
 else {
        result.cause().printStackTrace();
        tu.azzert(false,"Failed to listen");
      }
    }
  }
);
}
