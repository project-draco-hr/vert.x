{
  Class<?> c;
  if (loadFromModuleFirst) {
    try {
      c=loadFromModule(name);
    }
 catch (    ClassNotFoundException e) {
      c=platformClassLoader.loadClass(name);
    }
  }
 else {
    try {
      c=platformClassLoader.loadClass(name);
    }
 catch (    ClassNotFoundException e) {
      c=loadFromModule(name);
    }
  }
  if (resolve) {
    resolveClass(c);
  }
  return c;
}
