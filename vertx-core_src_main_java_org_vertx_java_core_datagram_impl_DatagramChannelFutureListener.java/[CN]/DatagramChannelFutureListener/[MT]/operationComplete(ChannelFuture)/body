{
  Channel ch=future.channel();
  if (context.isOnCorrectWorker(ch.eventLoop())) {
    try {
      vertx.setContext(context);
      notifyHandler(future);
    }
 catch (    Throwable t) {
      context.reportException(t);
    }
  }
 else {
    context.execute(new Runnable(){
      public void run(){
        notifyHandler(future);
      }
    }
);
  }
}
